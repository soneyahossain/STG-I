  %var_test = alloca i32, align 4                     ;v(var_test) -> address or    v(var_test) ->  (i32* 0x7f853cc03da0)       //should I also record type such as (i32* 0x7f853cc03da0) ??
  store i32* %var_test, i32** %ip_test, align 8       ;v(ip_test) -> v(var_test) -> (i32* 0x7f853cc03da0)
  %tmp_test1 = load i32*, i32** %ip_test, align 8     ;tmp_test1 = v(ip_test) --> (i32* 0x7f853cc03da0)
  %cmp_test = icmp ne i32* %tmp_test1, null           ;(ne (i32* 0x7f853cc03da0) null or 0?  )














